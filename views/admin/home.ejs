            </div>
            <div class="table-responsive">
              <table
                class="table text-start align-middle table-bordered table-hover mb-0"
              >
                <tr class="text-black">
                  <th>Name</th>
                  <th>Email</th>
                  <th>Mobile</th>
                  <th>Verified</th>
                  <th>User Login status</th>
                  <th>Action</th>
                </tr>

                <% if (users.length > 0) { %> <% for (let i = 0; i <
                users.length; i++) { %>
                <tr>
                  <td><%= users[i].name %></td>
                  <td><%= users[i].email %></td>
                  <td><%= users[i].mobile %></td>
                  <td>
                    <% if (users[i].is_verified === 0) { %>
                    <p>Not Verified</p>
                    <% } else { %>
                    <p>Verified</p>
                    <% } %>
                  </td>
                  <td>
                    <% if (users[i].isLoggedIn) { %>
                      <p>Logged In</p>
                    <% } else { %>
                      <p>Logged Out</p>
                    <% } %>
                  </td>
                  <td>  <a href="/admin/delete-user?id=<%= users[i]._id %>">Delete</a></td>
                 
                </tr>
                <%
             } %> <% } else { %>
                <tr>
                  <td colspan="4">Users Not Found</td>
                </tr>
                <% } %>
              </table>
            </div>
          </div>
