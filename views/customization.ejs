    <!-- query start -->

    <div class="queryLink">
      <div class="query1" id="showMoreBtn" onclick="showQueryForm()">
        <i class="bx bxl-gmail"></i>
      </div>
      <a href="https://wa.me/+61451496060" target="_blank">
        <div class="query2"><i class="bx bxl-whatsapp"></i></div
      ></a>
    </div>

    <!-- query end -->

    <!-- query form start -->
    <div class="queryForm" id="queryForm">
      <div class="background backgroundQuery">
        <div class="container">
          <div class="screen">
            <div class="screen-header">
              <div class="screen-header-left">
                <div class="screen-header-button close"></div>
                <div class="screen-header-button maximize"></div>
                <div class="screen-header-button minimize"></div>
              </div>
              <div class="screen-header-right">
                <a onclick="closeQueryBtn()">
                  <i class="bx bx-message-square-x"></i>
                </a>
              </div>
            </div>
            <div class="screen-body">
              <div class="screen-body-item left">
                <div class="app-title">
                  <span
                    >CONTACT
                    <span>US</span>
                  </span>
                </div>
                <div class="app-contact">CONTACT INFO : +61 451 496 060</div>
              </div>
              <div class="screen-body-item">
                <div class="app-form">
                  <div class="app-form-group">
                    <input
                      class="app-form-control"
                      value="Krishti Singh"
                      disabled
                    />
                  </div>
                  <div class="app-form-group">
                    <input
                      class="app-form-control"
                      placeholder="YOUR EMAIL"
                      required
                    />
                  </div>
                  <div class="app-form-group">
                    <input
                      class="app-form-control"
                      placeholder="CONTACT NO"
                      required
                    />
                  </div>
                  <div class="app-form-group message">
                    <input
                      class="app-form-control"
                      placeholder="MESSAGE"
                      required
                    />
                  </div>
                  <div class="app-form-group buttons">
                    <!-- <button class="app-form-button">CANCEL</button> -->
                    <button class="app-form-button">SEND</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- query form end -->

    <!-- Gallery show List Start -->
    <div id="moreGallery">
      <div class="container-fluid">
        <div class="row">
          <% products.forEach((product) => { %>
          <div class="col-md-4 gridGallery">
            <div class="product-item">
              <a href="/product/<%= product._id %>">
              <div class="product-image">
                  <img src="<%= product.image1 %>" alt="Image 1">
                  <div class="product-action">
                    <img src="<%= product.image2 %>" alt="Image 2">
                  </div>
                </div>
              </a>
              <div class="product-price">
                <%= product.title %>
              </div>
            </div>
          </div>
          <% }); %>  
        </div>
      </div>
    </div>
    <!-- Gallery show List End -->

