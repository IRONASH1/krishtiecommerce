    <!-- Wishlist Start -->
    <div class="wishlist-page">
      <div class="container-fluid">
        <div class="row">
          <% wishItems.forEach((wishItem) => { %>
          <div class="col-md-4 gridGallery">
            <div class="product-item">
              <div class="product-image" id="wishImg">
                <div class="img">
                  <a href="#"><img src="<%= wishItem.product.image1 %>" alt="Image1" /></a>
                </div>
              </div>
              <div class="product-price">
                <a><%= wishItem.product.title %></a><br>
                <a><i class='bx bx-money'></i>AUD <%= wishItem.totalPrice %></a>
                <div class="qty">
                  <!-- <button class="btn-minus">
                    <i class="fa fa-minus"></i>
                  </button>
                  <input type="text" value="1" />
                  <button class="btn-plus">
                    <i class="fa fa-plus"></i>
                  </button> -->
                  <form action="/removeWish/<%= wishItem.product.id %>" method="post" >
                    <input type="text" value="<%= wishItem.quantity %>" />
                      <button type="submit"><i class="fa fa-trash"></i></button>
                  </form>
                </div>
                <button class="btn-cart">Buy Now</button>
              </div>
            </div>
          </div>
          <% }); %>
        </div>
      </div>
    </div>
